{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"constants/Colors.js",
		"app/(drawer)/(tabs)/index.jsx",
		"app/(drawer)/(tabs)/_layout.jsx",
		"app/+not-found.jsx",
		"app/_layout.jsx"
	]
}
